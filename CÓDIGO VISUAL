                if (cuenta1[n].asig == 1) {
                    if (cuenta1[n].alumno == 1) {
                        printf("Nota: 5.6\n");
                       
                    }
                    if (cuenta1[n].alumno == 2) {
                        printf("Nota: 6\n");
                       
                    }
                    if (cuenta1[n].alumno == 3) {
                        printf("Nota: 4.5\n");
                       
                    }

                    printf("\tpulse 1 para volver al menu y otra tecla para cerrar el programa\n\n");
                    decision = leer_entero();
                    scanf_s("%d", &decision);
                    if (decision == 1)
                    {
                        system("cls");
                        cuenta1[n].op = 0;
                    }
                    else
                        exit(-1);
                }
                if (cuenta1[n].asig == 2) {
                    if (cuenta1[n].alumno == 1) {
                        printf("Nota: 7\n");

                    }
                    if (cuenta1[n].alumno == 2) {
                        printf("Nota: 5.8\n");

                    }
                    if (cuenta1[n].alumno == 3) {
                        printf("Nota: 8.3\n");

                    }
                    printf("\tpulse 1 para volver al menu y otra tecla para cerrar el programa\n\n");
                    decision = leer_entero();
                    scanf_s("%d", &decision);
                    if (decision == 1)
                    {
                        system("cls");
                        cuenta1[n].op = 0;
                    }
                    else
                        exit(-1);
                }
                if (cuenta1[n].asig == 3) {
                    if (cuenta1[n].alumno == 1) {
                        printf("Nota: 4.6\n");

                    }
                    if (cuenta1[n].alumno == 2) {
                        printf("Nota: 3.8\n");

                    }
                    if (cuenta1[n].alumno == 3) {
                        printf("Nota: 1.4\n");

                    }
                    printf("\tpulse 1 para volver al menu y otra tecla para cerrar el programa\n\n");
                    decision = leer_entero();
                    scanf_s("%d", &decision);
                    if (decision == 1)
                    {
                        system("cls");
                        cuenta1[n].op = 0;
                    }
                    else
                        exit(-1);
                }
                if (cuenta1[n].asig == 4) {
                    if (cuenta1[n].alumno == 1) {
                        printf("Nota: 5\n");

                    }
                    if (cuenta1[n].alumno == 2) {
                        printf("Nota: 4.9\n");

                    }
                    if (cuenta1[n].alumno == 3) {
                        printf("Nota: 5.5\n");

                    }
                    printf("\tpulse 1 para volver al menu y otra tecla para cerrar el programa\n\n");
