int main()
{
    setlocale(LC_CTYPE, "es-ES");
    Alumno uno;
    char puerto[] = "COM6";
    int  contador, dias_ingresado;
    int	dia_actual, mes_actual, hora_actual;
    for (contador = 0; contador < 3; contador++) //dia_actual, mes_actual, hora //este bucle for me extrae de la funcion time.h los valores que voy a usar en el main
    {
        if (contador == 0) {
            hora_actual = Obtener_hora_actual(contador);
        }

        if (contador == 1) {
            dia_actual = Obtener_hora_actual(contador);
        }
        if (contador == 2) {
            mes_actual = Obtener_hora_actual(contador);
        }

    }
    printf("%d/%d/%d\n", dia_actual,mes_actual,hora_actual);

    //Aqui irá arduino y declaración de variables

    char tecla;
    int opcion;
    Alumno cuenta1[TAM];
    Intro cuenta2[TAM];





    //Comienzo de lo que se ve en pantalla


    printf("=========Bienvenido a la ETSIDI=========\n\n Pulse cualquier tecla para continuar");

    tecla = _getch();

    system("cls"); //LImpia lo q hay en pantalla


    while (Arduino->IsConnected())
    {
        sprintf_s(secuencia, "%d", count);
        //gets_s(BufferSalida);
        //printf("Enviando %s\n", BufferSalida);
        char BufferSalida[200] = "tarjeta";
        Arduino->WriteData(BufferSalida, strlen(BufferSalida));
        count++;
        if (count == 1)
            Sleep(4000);
        else
            Sleep(4000);
        bytesRecibidos = Arduino->ReadData(BufferEntrada, sizeof(char) * 199);
        if (bytesRecibidos != -1)
        {
            BufferEntrada[bytesRecibidos - 1] = '\0';
            comp = strcmp(BufferEntrada, BufferSalida);
            if (comp == 1) {
                printf("No se ha recibido nada\n");
                Sleep(1000);
                system("cls");
            }
            else {
                do
                {
                    printf("Seleccione como quiere iniciar sesión:\n\n 1.Iniciar sesión como Administrador.\n 2.Iniciar sesión como estudiante.\n 3.Salir.");
                    Arduino = new Serial((char*)puerto);
                    opcion = leer_entero(); //funcion para elegir opcion

                    system("cls");

                    if (opcion == 1 || opcion == 2 || opcion == 3)//solo entra al menu si se introduce uno de estos valores
                    {
                        if (opcion == 1)
                        {
                            //printf("opcion1\n");

                            do
                            {

                                Alumno agenda[MAX];
                                Alumno evento[MAX];
                                int nAlumnos = 0;
                                int nAlumnos1 = 0;
                                int opcion1;

                                setlocale(LC_CTYPE, "es-ES");
                                nAlumnos = leer_fichero_txt(agenda);
                                nAlumnos1 = leer_fichero_txt1(evento);

                                do
                                {
                                    opcion1 = menu();
                                    switch (opcion1)
                                    {
                                    case 1:
                                        nAlumnos = alta_Alumno(agenda, nAlumnos);
                                        break;
                                    case 2:
                                        listado_agenda(agenda, nAlumnos);
                                        break;
                                    case 3:
                                        nAlumnos1 = alta_Evento(evento, nAlumnos1);

                                        break;
                                    case 4:
                                        listado_evento(evento, nAlumnos1);

                                        break;
                                    case 5:
                                        exit(-1);
                                        break;
                                    default:
                                        printf("Opción incorrecta\n");
                                        break;
                                    }
                                } while (opcion1 != 5);
                                crear_fichero_txt(agenda, nAlumnos);
                                crear_fichero_txt1(evento, nAlumnos1);

                                return 0;

                            } while (opcion == 1);



                        }
                        if (opcion == 2)
                        {
                            int nAlumnos = 0;
                            int x = 0;
                            int puntos;
                            Alumno* vector;
                            vector = (Alumno*)malloc(sizeof(Alumno) * puntos);
                            if (vector == NULL) {
                                printf("No hay espacio suficiente");
                            }
                            //creacion_usuario_2(cuenta2, nAlumnos);
                            printf("\n\tElija una opción de las siguientes: \n");
                            printf("\n\t 1.Iniciar sesión\n");
                            printf("\n\t 2.Crear cuenta\n");
                            scanf_s("%d", &x);
                            if (x == 1) {
                                intro_1(cuenta2, cuenta1);
                                menu_alumno(cuenta1, nAlumnos);
                            }
                            if (x == 2);
                            {
                                creacion_usuario_2(cuenta2, nAlumnos);
                                intro_1(cuenta2, cuenta1);
                                menu_alumno(cuenta1, nAlumnos);
                            }

                        }

                    }
                    else
                    {
                        printf("\n\n\nUsted ha introduciod un valor incorrecto, porfavor introduzca un valor válido\n\n\n");
                    }

                } while (opcion != 3);

                if (compruebafecha(cuenta1, tecla) == 1)
                {
                    printf("");
                }
            }
        }
    }
    
}

int leer_entero(void) { //con esta funcion evito sobrecargas al leer enteros y que quede en bucle el programa
    char cadena[2];
    int numero;
    gets_s(cadena);
    numero = atoi(cadena);//si hay un entero en la cadena se le asigana a num, sino, le asigna 0.
    return (numero);
}
int menu(void)
{
    int op;
    char intro;
    printf("\n");
    printf("1. Crear alumno\n");
    printf("2. Listado de Alumnos\n");
    printf("3. Crear evento\n");
    printf("4. Listado de eventos\n");
    printf("5. Salir del programa\n");
    printf("Opción=");
    scanf_s("%d", &op);
    scanf_s("%c", &intro, 1);
    return op;
}

void listado_agenda(Alumno a[], int n)
{
    int i;
    setlocale(LC_CTYPE, "es-ES");
    printf("\nEl contenido del listado es:\n");
    printf("\n=============================\n");
    for (i = 0; i < n; i++)
    {
        printf("Nombre =%s\n", a[i].nombre);
        printf("Número de matricula =%d\n", a[i].Num_matricula);
        printf("Titulación =%s\n", a[i].Titulacion);
        printf("Correo institucional =%s\n", a[i].correo);
        printf("==============\n");
    }
}
void listado_evento(Alumno b[], int n)
{
    int i;
    setlocale(LC_CTYPE, "es-ES");
    printf("\nLos eventos son:\n");
    printf("\n=============================\n");
    for (i = 0; i < n; i++)
    {
        printf("Nombre del evento =%s\n", b[i].evento);
        printf("Día =%d\n", b[i].dia);
        printf("Mes =%d\n", b[i].mes);
        printf("Hora =%d\n", b[i].hora);
        printf("==============\n");
    }
}

void crear_fichero_txt(Alumno a[], int n)
{
    FILE* fichero;
    int i;
    setlocale(LC_CTYPE, "es-ES");
    errno_t e;
    e = fopen_s(&fichero, "Agenda.txt", "wt");
    if (fichero == NULL)
        printf("No se ha podido guardar\n");
    else
    {
        for (i = 0; i < n; i++)
        {
            fprintf(fichero, "%s\n", a[i].nombre);
            fprintf(fichero, "%d\n", a[i].Num_matricula);
            fprintf(fichero, "%s\n", a[i].Titulacion);
            fprintf(fichero, "%s\n", a[i].correo);
        }
        fclose(fichero);
    }

}
void crear_fichero_txt1(Alumno b[], int n)
{
    FILE* fichero;
    int i;
    setlocale(LC_CTYPE, "es-ES");
    errno_t e;
    e = fopen_s(&fichero, "Evento.txt", "at");
    if (fichero == NULL)
        printf("No se ha podido guardar\n");
    else
    {
        for (i = 0; i < n; i++)
        {
            fprintf(fichero, "%s\n", b[i].evento);
            fprintf(fichero, "%d\n", b[i].dia);
            fprintf(fichero, "%d\n", b[i].mes);
            fprintf(fichero, "%d\n", b[i].hora);
        }
        fclose(fichero);
    }

}


int leer_fichero_txt(Alumno a[])
{
    setlocale(LC_CTYPE, "es-ES");
    FILE* fichero;
    int n = 0;
    errno_t e;
    char* p;
    e = fopen_s(&fichero, "Agenda.txt", "rt");
    if (fichero == NULL)
        printf("La lista estaba vacía\n");
    else
    {
        fgets(a[n].nombre, TAM, fichero);

        while (!feof(fichero))
        {
            p = strchr(a[n].nombre, '\n');
            *p = '\0';

            fscanf_s(fichero, "%d\n", &a[n].Num_matricula);
            fgets(a[n].Titulacion, TAM, fichero);
            fgets(a[n].correo, TAM, fichero);
            n++;
            fgets(a[n].nombre, TAM, fichero);
            fscanf_s(fichero, "%d\n", &a[n].Num_matricula);
            fgets(a[n].Titulacion, TAM, fichero);
            fgets(a[n].correo, TAM, fichero);
        }
        fclose(fichero);
    }
    return n;
}
int leer_fichero_txt1(Alumno b[])
{
    setlocale(LC_CTYPE, "es-ES");
    FILE* fichero;
    int n = 0;
    errno_t e;
    char* p;
    e = fopen_s(&fichero, "Evento.txt", "rt");
    if (fichero == NULL)
        printf("La lista estaba vacía\n");
    else
    {
        fgets(b[n].evento, TAM, fichero);

        while (!feof(fichero))
        {
            p = strchr(b[n].evento, '\n');
            *p = '\0';

            fscanf_s(fichero, "%d\n", &b[n].dia);
            fscanf_s(fichero, "%d\n", &b[n].mes);
            fscanf_s(fichero, "%d\n", &b[n].hora);
            n++;
            fgets(b[n].evento, TAM, fichero);
            fscanf_s(fichero, "%d\n", &b[n].dia);
            fscanf_s(fichero, "%d\n", &b[n].mes);
            fscanf_s(fichero, "%d\n", &b[n].hora);
        }
        fclose(fichero);
    }
    return n;
}
